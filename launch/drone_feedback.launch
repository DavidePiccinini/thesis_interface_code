<?xml version="1.0"?>
<launch>

  <!-- Define the csv file path (write only the prefix, the datetime and the csv extension will be added by the program) -->
  <param name="output_csv_path" value="$(find interface_code)/log/example"/>
  
  <!-- Start the drone feedback node -->
  <node name="drone_feedback" pkg="interface_code" type="drone_feedback.py" respawn="true" output="screen" />

</launch>
